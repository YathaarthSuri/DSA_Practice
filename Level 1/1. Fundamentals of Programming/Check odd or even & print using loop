import java.util.*;
  
  public class Main{
  
  public static void main(String[] args) {
      Scanner scn = new Scanner(System.in);
  
       // write ur code here
      //Scanner scn = new Scanner(System.in);
		int x = scn.nextInt();
		
		for(int i = 1; i <= x; i++){
		    if(i % 2 == 0){
		        System.out.println(i + " is even");
		    }
		    else{
		        System.out.println(i + " is odd");
		    }
		}
      
   }
  }
